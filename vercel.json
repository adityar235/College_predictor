{
    "version": 2,
    "builds": [
      {
        "src": "server.js",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      {
        "src": "/api/getColleges",
        "dest": "/server.js",
        "methods": ["POST"]
      },
      {
        "src": "/(.*)",
        "dest": "/index.html"
      }
    ]
  }
  
  